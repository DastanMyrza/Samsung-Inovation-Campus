import java.util.Scanner;
public class SIC_while {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
            int a,b=0,sum=0;
        a = in.nextInt();
            a = Math.abs(a);
        while (a>0){
            sum = a % 10;
            a/=10;

            b = b * 10;
            b = sum + b;
        }
        System.out.println(b);
    }
}
