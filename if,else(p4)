import java.util.Scanner;

public class SIC_if {
    public static void main(String[] args) {

        int a,n1,n2,n3;
        Scanner in = new Scanner(System.in);
        a = in.nextInt();
        n1 = a/100;
        n2 = a/10%10;
        n3 = a%10;
        if (n1 >= n2 && n1>=n3)
            System.out.println(n1);
        else if (n2 >= n1 && n2>=n3)
            System.out.println(n2);
        else
            System.out.println(n3);
    }
}
