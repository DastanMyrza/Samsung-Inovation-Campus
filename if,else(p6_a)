import java.util.Scanner;

public class SIC_if {
    public static void main(String[] args) {

        int n,n1;
        Scanner in = new Scanner(System.in);
        n = in.nextInt();
        n1 = n/10+n%10;

        if (n1>9){
            System.out.println("YES");
        }
        else
            System.out.println("NO");
    }
}
