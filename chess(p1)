import java.util.Scanner;

public class SIC_if {
    public static void main(String[] args) {
    Scanner in = new Scanner(System.in);
    int a,b;
        a = in.nextInt();
        b = in.nextInt();
        if ((a + b) % 2 ==1){
            System.out.println("Белый");
        }
        else
            System.out.println("Черный");
    }
}
