import java.util.Scanner;
public class SIC_while {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
            int a,b,sum=0;
        a = in.nextInt();
        b = in.nextInt();
            a = Math.abs(a);
            b = Math.abs(b);
        while (a>0){
            if (a%10 == b){
                sum++;
            }
            a/=10;
        }
        System.out.println(sum);
    }
}
